<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="net.neoturbine.autolycus" android:versionCode="1"
	android:versionName="1.0">
	<application android:icon="@drawable/icon" android:label="@string/app_name">



		<provider android:name=".providers.AutolycusProvider"
			android:syncable="false" android:authorities="net.neoturbine.providers.autolycus"></provider>
		<activity android:name="BusShortcuts">
			<intent-filter>
				<action android:name="android.intent.action.CREATE_SHORTCUT"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>
		<activity android:name="SelectRoute">
			<intent-filter>
				<action android:name="net.neoturbine.autolycus.SELECT_ROUTE"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>
		<activity android:name="SelectDirection">
			<intent-filter>
				<action android:name="net.neoturbine.autolycus.SELECT_DIRECTION"></action>
				<category android:name="android.intent.category.DEFAULT"></category>
			</intent-filter>
		</activity>
	</application>
	<uses-sdk android:minSdkVersion="3" />


	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
</manifest> 